# Supabase Configuration
# Get these from: https://supabase.com/dashboard/project/_/settings/api

# Database Connection String (from Project Settings -> Database -> Connection String -> URI)
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres

# Supabase Project URL (from Project Settings -> API -> Project URL)
SUPABASE_URL=https://[YOUR-PROJECT-REF].supabase.co

# Supabase Anon/Public Key (from Project Settings -> API -> Project API keys -> anon public)
SUPABASE_KEY=your-anon-key-here

# Application Settings
ENVIRONMENT=development
DEBUG=true
